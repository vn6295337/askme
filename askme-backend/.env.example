# AskMe Backend Proxy - Environment Configuration
# Copy this file to .env and add your actual API keys

# Server Configuration
PORT=3000
NODE_ENV=production

# AI Provider API Keys (Add your actual keys here)
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE
MISTRAL_API_KEY=YOUR_MISTRAL_API_KEY_HERE
LLAMA_API_KEY=YOUR_LLAMA_API_KEY_HERE

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Security Configuration
CORS_ORIGIN=*
TRUST_PROXY=false

# Logging Configuration
LOG_LEVEL=info
LOG_REQUESTS=true

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30000

# Provider Timeout Configuration
REQUEST_TIMEOUT_MS=30000

# Optional: Database Configuration for Analytics
# DATABASE_URL=postgresql://user:password@localhost:5432/askme_analytics

# Optional: Redis for Rate Limiting
# REDIS_URL=redis://localhost:6379
