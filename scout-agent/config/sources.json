{
  "github": {
    "enabled": true,
    "queries": [
      "language:Python topic:llm topic:free",
      "language:Python topic:language-model topic:open-source",
      "language:Python topic:transformer topic:free",
      "language:JavaScript topic:llm topic:free",
      "topic:large-language-model topic:free"
    ],
    "perPage": 30
  },
  "huggingface": {
    "enabled": true,
    "baseUrl": "https://huggingface.co/api/models",
    "filters": {
      "pipeline_tag": "text-generation",
      "sort": "downloads",
      "direction": -1,
      "limit": 30
    },
    "reputableOrganizations": [
      "microsoft", "google", "meta-llama", "mistralai", "microsoft",
      "nvidia", "stabilityai", "huggingface", "databricks", "togethercomputer",
      "bigscience", "EleutherAI", "teknium", "NousResearch", "WizardLM",
      "openchat", "lmsys", "argilla", "upstage"
    ]
  },
  "arxiv": {
    "enabled": false,
    "baseUrl": "http://export.arxiv.org/api/query",
    "searchTerms": [
      "large language model",
      "transformer language model",
      "llm",
      "open source language model",
      "free language model"
    ],
    "maxResults": 20
  },
  "papersWithCode": {
    "enabled": false,
